Algoritmo "AUTOESCOLA"

Var
   N1 , N2 , M: real

   
   


Inicio
      escreval ("AUTOESCOLA")
      escreva ("Primeira nota: ")
      leia (N1)
      escreva ("Segunda nota: ")
      leia (N2)
      M <- (N1 + N2) / 2
      escreval ("Média: ", M)
      se (M >= 7) entao
         escreva ("Aprovado")
      senao
           escreva ("Reprovado")
      fimse




Fimalgoritmo