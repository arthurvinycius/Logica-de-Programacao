Algoritmo "Conversão_real_dolar"

Var
   C, Q: inteiro
   R, D: real

   
   


Inicio
      C <- 1
      escreva ("Quantas vezes vc quer converter? ")
      leia (Q)
      enquanto (C <= Q) faca
        escreva ("Qual Valor a ser convertido? R$")
        leia (R)
        D <- R / 5.39
        escreval ("Então eu tenho Us$", D:5:2)
        C <- C + 1
      fimenquanto




Fimalgoritmo